{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "shru1dfpractice"
		},
		"ls_AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_AzureSqlDatabase1'"
		},
		"ls_SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_SqlServer1'"
		},
		"ls_Sql_Server_OnPrem_Dynamic_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_Sql_Server_OnPrem_Dynamic'",
			"defaultValue": "Integrated Security=True;Data Source=@{linkedService().servename};Initial Catalog=@{linkedService().db}"
		},
		"ls_adlsgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adlsgen2'"
		},
		"ls_AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "shrusqlserver.database.windows.net"
		},
		"ls_AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "shrusqldatabase"
		},
		"ls_AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "shruadmin"
		},
		"ls_Sql_Server_OnPrem_Dynamic_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "@{linkedService().username}"
		},
		"ls_adlsgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sashrudevenv.dfs.core.windows.net/"
		},
		"TumblingWindow_properties_pipeline_parameters_servename": {
			"type": "string",
			"defaultValue": "CHANDANCS\\SQLEXPRESS"
		},
		"TumblingWindow_properties_pipeline_parameters_db": {
			"type": "string",
			"defaultValue": "C_Tech"
		},
		"TumblingWindow_properties_pipeline_parameters_username": {
			"type": "string",
			"defaultValue": "CHANDANCS\\ChandanS"
		},
		"TumblingWindow_properties_pipeline_parameters_password": {
			"type": "string",
			"defaultValue": "Amma@143"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "shru1dfpractice_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AzureSqlDatabase1_password": {
						"value": "[parameters('ls_AzureSqlDatabase1_password')]"
					},
					"ls_adlsgen2_accountKey": {
						"value": "[parameters('ls_adlsgen2_accountKey')]"
					},
					"ls_SqlServer1_connectionString": {
						"value": "[parameters('ls_SqlServer1_connectionString')]"
					},
					"ls_Sql_Server_OnPrem_Dynamic_connectionString": {
						"value": "[parameters('ls_Sql_Server_OnPrem_Dynamic_connectionString')]"
					},
					"ls_AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('ls_AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"ls_AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('ls_AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"ls_AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('ls_AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"ls_adlsgen2_properties_typeProperties_url": {
						"value": "[parameters('ls_adlsgen2_properties_typeProperties_url')]"
					},
					"ls_Sql_Server_OnPrem_Dynamic_properties_typeProperties_userName": {
						"value": "[parameters('ls_Sql_Server_OnPrem_Dynamic_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "shru1dfpractice_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'shru1dfpractice_ArmTemplate_0')]"
			]
		},
		{
			"name": "shru1dfpractice_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"TumblingWindow_properties_pipeline_parameters_servename": {
						"value": "[parameters('TumblingWindow_properties_pipeline_parameters_servename')]"
					},
					"TumblingWindow_properties_pipeline_parameters_db": {
						"value": "[parameters('TumblingWindow_properties_pipeline_parameters_db')]"
					},
					"TumblingWindow_properties_pipeline_parameters_username": {
						"value": "[parameters('TumblingWindow_properties_pipeline_parameters_username')]"
					},
					"TumblingWindow_properties_pipeline_parameters_password": {
						"value": "[parameters('TumblingWindow_properties_pipeline_parameters_password')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'shru1dfpractice_ArmTemplate_1')]"
			]
		}
	]
}